{
    "name": "cooperl/laravel-db2",
    "description": "laravel-db2 is a simple DB2 service provider for Laravel. It provides DB2 Connection by extending the Illuminate Database component of the laravel framework.",
    "keywords": [
        "laravel",
        "DB2",
        "Database",
        "PDO",
        "ODBC"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Maxime Rault",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0|^8.1|^8.2",
        "illuminate/database": "^10.3"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "Cooperl\\DB2\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cooperl\\DB2\\DB2ServiceProvider"
            ]
        }
    }
}
